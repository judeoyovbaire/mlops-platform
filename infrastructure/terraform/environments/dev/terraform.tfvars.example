# Example terraform.tfvars for development environment
# Copy this file to terraform.tfvars and update values

aws_region     = "eu-west-1"
cluster_name   = "mlops-platform-dev"
cluster_version = "1.33"

# VPC configuration
vpc_cidr        = "10.0.0.0/16"
private_subnets = ["10.0.1.0/24", "10.0.2.0/24", "10.0.3.0/24"]
public_subnets  = ["10.0.101.0/24", "10.0.102.0/24", "10.0.103.0/24"]

# MLflow database password (use a strong password)
mlflow_db_password = "CHANGE_ME_TO_SECURE_PASSWORD"

# Additional tags
tags = {
  Environment = "dev"
  Project     = "mlops-platform"
  Owner       = "your-name"
}